<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.equipealpha.univagas">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MeusDadosActivity"
            android:label="@string/label_MeusDados"
            android:theme="@style/AppTheme.NoActionBar"
            >
        </activity>

        <activity
            android:name=".EditarDadosPessoaisActivity"
            android:label="@string/label_EditarDadosPessoais"
            />

        <activity
            android:name=".EditarSenhaActivity"
            android:label="@string/label_EditarSenha"
            />

        <!-- <activity android:name=".MeusDadosActivity"></activity> -->
        <activity
            android:name=".EscolherContaActivity"
            android:label="@string/label_EscolherConta"
            />

        <activity
            android:name=".CriarContaActivity"
            android:label="@string/label_CriarConta"
            />

        <activity android:name=".LoginActivity">

        </activity>

    </application>

</manifest>